TS24501IES DEFINITIONS AUTOMATIC TAGS ::=
BEGIN

IMPORTS
ExtQualityOfService, MCCMNCPair, IMEI, IMEISV, TFT, TimeZoneAndTime,
GPRSTimer, GPRSTimer3, TMGI
FROM TS24008IES

IMPORTS
APNAggMaxBitRate, EPSQualityOfService, ExtAPNAggMaxBitRate, TimeDuration
FROM TS24301IES
;


/* -- TYPE DEFINITIONS --- */

/* OpaqueData can be used for any case where the specification has
   some number of octets preceded by a 2-octet length. */
OpaqueData ::= SEQUENCE {
   --<is3GLength bitFieldSize="16"/>
   length INTEGER (1..65535),
   --<length fixed-size="0"/>
   contents OCTET STRING  
}


/* COMMON INFORMATION ELEMENTS */

/* 9.11.2.1 - Additional information */
AdditionalInfo ::= OCTET STRING

/* 9.11.2.1A - Access type */
AccessType ::= SEQUENCE {
   spare INTEGER (0..3) DEFAULT 0,
   typeOfAccess ENUMERATED {
      access3gpp(1), accessNon3gpp(2)
   }
}

/* 9.11.2.1B - DNN */
DNNList ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255),
   --<length fixed-size="0"/>
   apn SEQUENCE OF IA5String
}

/* 9.11.2.2 - EAP message */
EAPMessage ::= SEQUENCE {
   --<is3GLength bitFieldSize="16"/>
   length INTEGER (1..65535),
   --<asn1typename>EAPPacketFormat</asn1typename>
   packetFormat SEQUENCE { -- defined in IETF RFC 3748, Section 4
      code EAPFormatCode,
      id INTEGER (0..255),
      length INTEGER (4..65535),
      --<length expr="@.length - 4"/>
      data OCTET STRING
   }
}

EAPFormatCode ::= ENUMERATED {
   request(1), response(2), success(3), failure(4),
   codeMax(255)
}

/* 9.11.2.6 - Intra N1 mode NAS transparent container */
IntraN1NASContainer ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255),
   authCode OCTET STRING (SIZE (4)),
   cipherType INTEGER (0..15),
   integrityType INTEGER (0..15),
   spare1 INTEGER (0..7) DEFAULT 0,
   kacf BOOLEAN,
   tsc BOOLEAN,
   keysetId INTEGER (0..7),
   seqNumber INTEGER (1..255)
}

/* 9.11.2.7 - N1 mode to S1 mode NAS transparent container */
N1ToS1NASContainer ::= INTEGER (1..255)

/* 9.11.2.8 - S-NSSAI */
SNSSAI ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255),
   sst SST,
   --<noPresentBit/>
   sd SD OPTIONAL,
   --<noPresentBit/>
   mappedSST OCTET STRING (SIZE (1)) OPTIONAL,
   --<noPresentBit/>
   mappedSD OCTET STRING (SIZE (3)) OPTIONAL
}

SST ::= INTEGER (1..255)

SD ::= INTEGER (1..16777215)

/* 9.11.2.9 - S1 mode to N1 mode NAS transparent container */
S1ToN1NASContainer ::= SEQUENCE {   
   --<is3GLength/>
   length INTEGER (1..255),
   authCode OCTET STRING (SIZE (4)),
   cipherType INTEGER (0..15),
   integrityType INTEGER (0..15),
   spare1 BOOLEAN DEFAULT FALSE,
   ncc INTEGER (0..7),
   tsc BOOLEAN,
   keysetId INTEGER (0..7),
   spare2 INTEGER (1..65535) DEFAULT 0
}

/* 9.11.2.10 - Service-level-AA container - added in 17.3 */
SLAAContainer ::= SEQUENCE {
   --<is3GLength bitFieldSize="16"/>
   length INTEGER (0..65535),
   --<is3GVarLenList lengthElem="length"/>
   contents SEQUENCE OF SLAAParameter
}

SLAAParameter ::= SEQUENCE {
   type SLAAParamTypes,
   --<inline/>
   param CHOICE {
      --<selector element="type" value="10"/>
      pendingInd SLAAPendingIndication,
      --<selector element="type" value="16"/>
      deviceId SLDeviceId,
      --<selector element="type" value="32"/>
      serverAddr SLAAServerAddr,
      --<selector element="type" value="48"/>
      response SLAAResponse,
      --<selector element="type" value="64"/>
      payloadType SLAAPayloadType,
      --<selector element="type" value="80"/>
      statusInd SLAAStatusIndication,
      --<selector element="type" value="112"/>
      payload SLAAPayload
   }
}

SLAAParamTypes ::= ENUMERATED {
   ptPendingInd(10), ptDeviceId(16), ptServerAddr(32), ptResponse(48),
   ptPayloadType(64), ptStatusInd(80), ptPayload(112), ptMax(255)
}

/* 9.11.2.11 - Service-level device ID - added in 17.3 */
SLDeviceId ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255),
   id OCTET STRING (SIZE (1..255))
}

/* 9.11.2.12 - Service-level-AA server address - added in 17.3 */
SLAAServerAddr ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255),
   addrType INTEGER (1..255),
   --<inline/>
   addr CHOICE { -- Changed in v18.10
      --<selector element="addrType" value="1"/>
      addrIpv4 OCTET STRING (SIZE (4)),
      --<selector element="addrType" value="2"/>
      addrIpv6 OCTET STRING (SIZE (16)),
      --<selector element="addrType" value="3"/>
      --<inline/>
      addrIpv4v6 SEQUENCE {
         ipv4 OCTET STRING (SIZE (4)),
         ipv6 OCTET STRING (SIZE (16))
      },
      --<selector element="addrType" value="4"/>
      --<inline/>
      addrFQDN SEQUENCE {
         length INTEGER (0..255),
         fqdn PrintableString
      }
   }
}

/* 9.11.2.13 - Service-level-AA payload - added in 17.3 */
SLAAPayload ::= SEQUENCE {
   --<is3GLength bitFieldSize="16"/>
   length INTEGER (0..65535),
   payload OCTET STRING (SIZE (1..65532))
}

/* 9.11.2.14 - Service-level-AA response - added in 17.3 */
SLAAResponse ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255),
   spare INTEGER (0..15) DEFAULT 0,
   c2ar INTEGER (0..3), -- added in v17.6
   slar INTEGER (0..3) -- type modified in v17.6
}

/* 9.11.2.15 - Service-level-AA payload type - added in 17.5 */
SLAAPayloadType ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255),
   type SLAAPayloadTypes
}

SLAAPayloadTypes ::= ENUMERATED {
   pltUUAA(1), pltc2Auth(2), pltMax(255)
}

/* 9.11.2.17 - Service-level-AA pending indication - added in 17.6 */
SLAAPendingIndication ::= SEQUENCE {
   spare1 INTEGER (0..15) DEFAULT 0, /* lack of 3GLength in this struct */
   spare2 INTEGER (0..7) DEFAULT 0,
   slapi BOOLEAN
}

/* 9.11.2.18 - Service-level-AA service status indication - added in 17.9 */
SLAAStatusIndication ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255),
   spare INTEGER (0..127) DEFAULT 0,
   uas BOOLEAN
}


/* MOBILITY MANAGEMENT INFORMATION ELEMENTS */

/* 9.11.3.1 - MM capability */
MMCapability ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255) DEFAULT 1,

   /* octet 3 */
   sgc BOOLEAN,
   hcCpCIoT BOOLEAN,
   n3Data BOOLEAN,
   cpCIoT BOOLEAN,
   restrictedEC BOOLEAN,
   lppSupported BOOLEAN,
   handoverSupported BOOLEAN,
   s1Supported BOOLEAN,
   --<noPresentBit/>
   octet4 SEQUENCE {
      racs BOOLEAN,
      nssaa BOOLEAN,
      lcs BOOLEAN,
      v2xcnpc5 BOOLEAN,
      v2xcepc5 BOOLEAN,
      v2x BOOLEAN,
      upCIoT BOOLEAN,
      srvcc BOOLEAN
   } OPTIONAL,
   --<noPresentBit/>
   octet5 SEQUENCE {
      proSel2relay BOOLEAN, -- Added in v17.3
      proSedc BOOLEAN, -- Added in v17.3
      proSedd BOOLEAN, -- Added in v17.3
      erNssai BOOLEAN,  -- Added in v17.0
      ehcCpCIoT BOOLEAN,  -- Added in v16.5
      multipleUp BOOLEAN, -- Added in v16.5
      wusa BOOLEAN,
      cag BOOLEAN
   } OPTIONAL,
   --<noPresentBit/>
   octet6 SEQUENCE { -- Added in v17.3
      pr BOOLEAN, -- Added in v17.5
      rpr BOOLEAN, -- Added in v17.5
      piv BOOLEAN, -- Added in v17.5
      ncr BOOLEAN, -- Added in v17.5
      nrpssi BOOLEAN, -- Added in v17.5
      proSel3rmt BOOLEAN,
      proSel2rmt BOOLEAN,
      proSel3relay BOOLEAN
   } OPTIONAL,
   --<noPresentBit/>
   octet7 SEQUENCE { -- Added in v17.5
      mpsiu BOOLEAN, -- Added in v18.1, renamed in v18.4
      uas BOOLEAN, -- Added in v18.1, renamed in v18.4
      nsag BOOLEAN, -- Added in v17.7
      exCag BOOLEAN, -- Added in v17.6
      sorSnpnSi BOOLEAN, -- Added in v17.6
      eventNotify BOOLEAN, -- Added in v17.6
      mint BOOLEAN,
      nssrg BOOLEAN
   } OPTIONAL,
   --<noPresentBit/>
   octet8 SEQUENCE { -- Added in v18.1
      sbts BOOLEAN, -- Added in v18.2
      nsr BOOLEAN, -- Added in v18.2
      ladnds BOOLEAN, -- Added in v18.2
      ranTiming BOOLEAN, -- Added in v18.2
      eci BOOLEAN, -- Added in v18.2
      esie BOOLEAN, -- Added in v18.2, renamed in v18.4
      rcman BOOLEAN, -- Renamed in v18.4
      rcmap BOOLEAN -- Renamed in v18.4
   } OPTIONAL,
   --<noPresentBit/>
   octet9 SEQUENCE { -- Added in v18.2
      proSel2End BOOLEAN, -- Added in v18.3, moved in v18.4
      proSel3U2URelay BOOLEAN, -- Added in v18.3, moved in v18.4
      proSel2U2URelay BOOLEAN, -- Added in v18.4
      slpspc5 BOOLEAN, -- Added in v18.4
      sbns BOOLEAN, -- Added in v18.2, moved in v18.4
      unper BOOLEAN, -- Added in v18.1, moved in v18.4
      a2xnpc5 BOOLEAN,
      a2xepc5 BOOLEAN
   } OPTIONAL,
   --<noPresentBit/>
   octet10 SEQUENCE { -- Added in v18.4
      a2xuu BOOLEAN, -- Added in v18.5
      slvi BOOLEAN,
      tempNS BOOLEAN,
      supl BOOLEAN,
      lcsupp BOOLEAN,
      pns BOOLEAN,
      rsppc5 BOOLEAN, -- Added in v18.3, moved in v18.4
      proSel3End BOOLEAN -- Added in v18.3, moved in v18.4
   } OPTIONAL,
   --<noPresentBit/>
   octet11 SEQUENCE { -- Added in v18.5
      spare INTEGER (0..15) DEFAULT 0, 
      nsuc BOOLEAN, -- Added in v18.6
      rslpl BOOLEAN, -- Added in v18.6
      nvlsatnr BOOLEAN, -- Added in v18.6
      mcsiu BOOLEAN
   } OPTIONAL,
   --<noPresentBit/>
   --<length fixed-size="0"/>
   spare OCTET STRING OPTIONAL
}

/* 9.11.3.2 - MM cause */
MMCause ::= INTEGER (0..255)

/* 9.11.3.2A - DRX parameters */
DRXParams ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1), 
   spare INTEGER (0..15) DEFAULT 0, 
   drxValue INTEGER (0..15)
}

/* 9.11.3.3 - Identity type */
IdentityType ::= SEQUENCE {
   spare BOOLEAN DEFAULT FALSE,
   typeOfIdent TypeOfIdentity
}

TypeOfIdentity ::= ENUMERATED {
   none(0), suci(1), guti(2), imei(3), stmsi(4), imeisv(5), macAddr(6),
   eui64(7) }

/* 9.11.3.4 - 5G mobile identity */
MobileIdentity ::= CHOICE {
   none NULL,   
   suci CHOICE {
      -- The supiFormat bit in the MobileIdentity determines which of these
      -- choices is present in an encoding.
      supiImsi MobileIdentity_SUCI_IMSI,
      supiNetSpecificId MobileIdentity_SUCI_NetId
   },
   guti MobileIdentity_GUTI,
   imei IMEI,
   stmsi MobileIdentity_STMSI,
   imeisv IMEISV,
   mac MobileIdentity_MAC,
   eui-64 OCTET STRING (SIZE(8))
}

MobileIdentity_GUTI ::= SEQUENCE {
   mcc-mnc MCCMNCPair,
   amfRegionID INTEGER (0..255),
   amfSetID INTEGER (0..1023),
   amfPointer INTEGER (0..63),
   tmsi OCTET STRING (SIZE (4))
}

MobileIdentity_STMSI ::= SEQUENCE {
   amfSetID INTEGER (0..1023),
   amfPointer INTEGER (0..63),
   tmsi OCTET STRING (SIZE (4))
}

MobileIdentity_SUCI_IMSI ::= SEQUENCE {
   mcc-mnc MCCMNCPair,
   routingInd2 INTEGER (0..15),
   routingInd1 INTEGER (0..15),
   routingInd4 INTEGER (0..15),
   routingInd3 INTEGER (0..15),
   spare INTEGER (0..15) DEFAULT 0,
   protSchemeId INTEGER (0..15),
   homeNetId INTEGER (0..255),
   --<length fixed-size="0"/>
   schemeOutput OCTET STRING
}

MobileIdentity_SUCI_NetId ::= SEQUENCE {
   --<length fixed-size="0">
   nai OCTET STRING
}

MobileIdentity_MAC ::= SEQUENCE {
   mauri BOOLEAN,
   macaddr OCTET STRING (SIZE(6))
}

/* 9.11.3.5 - 5GS network feature support */
NetworkFeatureSupport ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255),

   /* octet 3 */
   mpsInd BOOLEAN, 
   interNetworkWithoutN26Ind BOOLEAN,
   emfInd INTEGER (0..3), 
   emcInd INTEGER (0..3), 
   imsVoPSN3GPPInd BOOLEAN,
   imsVoPS3GPPInd BOOLEAN,

   --<noPresentBit/>
   /* octet 4 */
   octet4 SEQUENCE {
      upCIoT BOOLEAN,
      hcCpCIoT BOOLEAN,
      n3Data BOOLEAN,
      cpCIoT BOOLEAN,
      restrictedEC INTEGER (0..3),
      mcsi BOOLEAN,
      emcn3 BOOLEAN
   } OPTIONAL,

   --<noPresentBit/>
   /* octet 5 */
   octet5 SEQUENCE {
      unper BOOLEAN, -- Added in v18.1
      pr BOOLEAN, -- Added in v17.3
      rpr BOOLEAN, -- Added in v17.3
      piv BOOLEAN, -- Added in v17.3
      ncr BOOLEAN, -- Added in v17.3
      ehcCpCIoT BOOLEAN, -- Added in v16.5
      atsInd BOOLEAN,
      lcs BOOLEAN        -- Added in v16.5
   } OPTIONAL,

   --<noPresentBit/>
   /* octet 6 */
   octet6 SEQUENCE { -- Added in v18.3
      spare INTEGER (0..15) DEFAULT 0,
      rslp BOOLEAN, -- Added in v18.5
      supl BOOLEAN, -- Added in v18.4
      lcsupp BOOLEAN, -- Added in v18.4
      naps BOOLEAN
   } OPTIONAL
}

/* 9.11.3.6 - Registration result */
RegistrationResult ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255),
   spare BOOLEAN DEFAULT FALSE,
   disasterRegisterValue BOOLEAN, -- Added in v17.6
   emergencyRegistered BOOLEAN,
   nssaa BOOLEAN,
   smsAllowed BOOLEAN,
   regResult INTEGER (0..7)
}

/* 9.11.3.7 - Registration type */
RegistrationType ::= SEQUENCE {
   forBit BOOLEAN,
   regisType RegistrationTypeValue
}

RegistrationTypeValue ::= ENUMERATED {
   initial(1), mobilityUpdate(2), periodicUpdate(3), emergency(4),
   snpnOnboard(5), disasterUpdate(6), disasterInitial(7)
} 

/* 9.11.3.8 - Tracking area identity */
/* Mostly the same as 24.301 except the TAC is 3 octets instead of 2... */
TrackingAreaIdent ::= SEQUENCE {
   mcc-mnc MCCMNCPair,
   tac TrackingAreaCode
}

TrackingAreaCode ::= OCTET STRING (SIZE (3))

/* 9.11.3.9 - Tracking area identity list */
/* Mostly the same as 24.301 except the TAC is 3 octets instead of 2... */
/* Also used for Service Area List type (9.11.3.49)... */
TrackingAreaIdentList ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255), 
   --<is3GVarLenList lengthElem="length"/>
   idents SEQUENCE (SIZE (1..16)) OF TrackingAreaIdentListItem
}

TrackingAreaIdentListItem ::= SEQUENCE {
   allowedType BOOLEAN, -- Used by ServiceAreaList (9.11.3.49)
   typeOfList INTEGER (0..3),
   numElements INTEGER (0..31),  -- number of elements - 1
   --<inline/>
   elements CHOICE {
      --<selector element="typeOfList" value="0"/>
      --<asn1typename>PartialTrkAreaIdent0</asn1typename>
      partTrkIdentList0 SEQUENCE {
         mcc-mnc MCCMNCPair,
         --<inline/>
         --<length expr="@...numElements + 1"/>
         tacs SEQUENCE (SIZE (1..32)) OF TrackingAreaCode
      },      
      --<selector element="typeOfList" value="1"/>
      --<asn1typename>PartialTrkAreaIdent1</asn1typename>
      partTrkIdentList1 SEQUENCE {
         mcc-mnc MCCMNCPair,
         tac TrackingAreaCode
      },
      --<selector element="typeOfList" value="2"/>
      --<length expr="@.numElements + 1"/>
      --<asn1typename>PartialTrkAreaIdent2</asn1typename>
      partTrkIdentList2 SEQUENCE (SIZE (1..32)) OF TrackingAreaIdent,
      --<selector element="typeOfList" value="3"/>
      partTrkIdentList3 MCCMNCPair -- Used by ServiceAreaList (9.11.3.49)
   }
}

/* 9.11.3.9A - Update type */
UpdateType  ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255),
   spare BOOLEAN DEFAULT FALSE,
   spare2 BOOLEAN DEFAULT FALSE, -- "clie" bit removed in v18.5
   epsPnbCIoT INTEGER (0..3),
   pnbCIoT INTEGER (0..3),
   ngranUpd BOOLEAN,
   smsReq BOOLEAN
}

/* 9.11.3.10 - ABBA */
ABBA ::= OCTET STRING  -- (preceeded by 1 octet length)

/* 9.11.3.12 - Additional security information */
AddlSecurityInfo ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255),
   spare INTEGER (0..63) DEFAULT 0,
   rinmr BOOLEAN,
   hdp BOOLEAN
}

/* 9.11.3.12A - Additional information requested */
AdditionalInfoRequested ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255),
   spare INTEGER (0..127) DEFAULT 0,
   cipherKey BOOLEAN
}

/* 9.11.3.13 - Allowed PDU session status */
AllowedPDUSessionStatus ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255),
   psi-7 BOOLEAN,
   psi-6 BOOLEAN,
   psi-5 BOOLEAN,
   psi-4 BOOLEAN,
   psi-3 BOOLEAN,
   psi-2 BOOLEAN,
   psi-1 BOOLEAN,
   psi-0 BOOLEAN DEFAULT FALSE,
   psi-15 BOOLEAN,
   psi-14 BOOLEAN,
   psi-13 BOOLEAN,
   psi-12 BOOLEAN,
   psi-11 BOOLEAN,
   psi-10 BOOLEAN,
   psi-9 BOOLEAN,
   psi-8 BOOLEAN,
   --<noPresentBit/>
   spare OCTET STRING OPTIONAL
}

/* 9.11.3.18 - Configuration update indication */
ConfigUpdateIndication ::= SEQUENCE {
   spare INTEGER (0..3) DEFAULT 0,
   regRequested BOOLEAN,
   ack BOOLEAN
}

/* 9.11.3.18A - CAG information list */
CAGInfoList ::= SEQUENCE {
   --<is3GLength bitFieldSize="16"/>
   length INTEGER (0..65535),
   --<is3GVarLenList lengthElem="length"/>
   --<noPresentBit/>
   entries SEQUENCE OF CAGEntry OPTIONAL
}

CAGEntry ::= SEQUENCE {
   length INTEGER (1..255),
   mcc-mnc MCCMNCPair,
   spare INTEGER (0..127) DEFAULT 0,
   cagOnly BOOLEAN,
   --<noPresentBit/>
   --<length fixed-size="0"/>
   cagID OCTET STRING OPTIONAL
}

/* 9.11.3.18B - CIoT small data container */
CIoTSmallDataContainer ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255),
   --<is3GVarLenList lengthElem="length"/>
   --<storage>array</storage>
   dataType CIoTDataTypes,
   --<inline/>
   containerContents CHOICE {
      --<selector element="dataType" value="0"/>
      ctrlPlaneUserData SEQUENCE {
         ddx INTEGER (0..3),
         pduSessionId INTEGER (0..7)
      },
      --<selector element="dataType" value="1"/>
      sms SEQUENCE {
         spare INTEGER (0..31) DEFAULT 0
      },
      --<selector element="dataType" value="2"/>
      locSvcsMsgContainer SEQUENCE {
         ddx INTEGER (0..3),
         spare INTEGER (0..7) DEFAULT 0,
         addlInfoLen INTEGER (0..255),
         --<noPresentBit/>
         --<length expr="@.addlInfoLen"/>
         addlInfo OCTET STRING OPTIONAL
      }
   },
   --<length fixed-size="0"/>
   contents OCTET STRING
}

CIoTDataTypes ::= ENUMERATED {
   ciotCtrlPlaneUserData(0), ciotSMS(1), ciotLocSvcsMsg(2), ciotMAX(7)
} 

/* 9.11.3.18C - Ciphering key data */
CipheringKeyData ::= SEQUENCE {
   --<is3GLength bitFieldSize="16"/>
   length INTEGER (1..65535),
   --<is3GVarLenList lengthElem="length"/>
   dataSet SEQUENCE OF CipherDataSet
}

CipherDataSet ::= SEQUENCE {
   cipherId INTEGER (0..65535),
   cipherKey OCTET STRING (SIZE (16)),
   spare1 INTEGER (0..7) DEFAULT 0,
   c0Len INTEGER (0..31),
   --<length expr="@c0Len"/>
   c0 OCTET STRING,
   spare2 INTEGER (0..15) DEFAULT 0,
   eutraPosSIB SEQUENCE {
      --<is3GLength bitFieldSize="4"/>
      length INTEGER (0..15),
      --<noPresentBit/>
      octet1 SEQUENCE {
         posSIBType1-1 BOOLEAN,
         posSIBType1-2 BOOLEAN,
         posSIBType1-3 BOOLEAN,
         posSIBType1-4 BOOLEAN,
         posSIBType1-5 BOOLEAN,
         posSIBType1-6 BOOLEAN,
         posSIBType1-7 BOOLEAN,
         posSIBType1-8 BOOLEAN
      } OPTIONAL,
      --<noPresentBit/>
      octet2 SEQUENCE {
         posSIBType2-1 BOOLEAN,
         posSIBType2-2 BOOLEAN,
         posSIBType2-3 BOOLEAN,
         posSIBType2-4 BOOLEAN,
         posSIBType2-5 BOOLEAN,
         posSIBType2-6 BOOLEAN,
         posSIBType2-7 BOOLEAN,
         posSIBType2-8 BOOLEAN
      } OPTIONAL,
      --<noPresentBit/>
      octet3 SEQUENCE {
         posSIBType2-9 BOOLEAN,
         posSIBType2-10 BOOLEAN,
         posSIBType2-11 BOOLEAN,
         posSIBType2-12 BOOLEAN,
         posSIBType2-13 BOOLEAN,
         posSIBType2-14 BOOLEAN,
         posSIBType2-15 BOOLEAN,
         posSIBType2-16 BOOLEAN
      } OPTIONAL,
      --<noPresentBit/>
      octet4 SEQUENCE {
         posSIBType2-17 BOOLEAN,
         posSIBType2-18 BOOLEAN,
         posSIBType2-19 BOOLEAN,
         posSIBType2-20 BOOLEAN,
         posSIBType2-21 BOOLEAN,
         posSIBType2-22 BOOLEAN,
         posSIBType2-23 BOOLEAN,
         posSIBType2-24 BOOLEAN
      } OPTIONAL,
      --<noPresentBit/>
      octet5 SEQUENCE {
         posSIBType2-25 BOOLEAN,
         posSIBType3-1 BOOLEAN,
         posSIBType4-1 BOOLEAN,
         posSIBType5-1 BOOLEAN,
         posSIBType1-9 BOOLEAN, -- added in v17.13
         posSIBType1-10e BOOLEAN, -- added in v17.13
         spare INTEGER(0..3) DEFAULT 0
      } OPTIONAL
   },
   spare3 INTEGER (0..15) DEFAULT 0,
   nrPosSIB SEQUENCE {
      --<is3GLength bitFieldSize="4"/>
      length INTEGER (0..15),
