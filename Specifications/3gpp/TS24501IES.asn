N3QAIParam ::= SEQUENCE {
   paramId INTEGER (0..255),
   --<is3GLength/>
   length INTEGER (1..255),
   --<inline/>
   contents CHOICE {
      --<selector element="paramId" value="1"/>
      fiveQI QOSFlowDescriptions,
      --<selector element="paramId" value="2"/>
      gfbrUL QOSFlowDescriptions,
      --<selector element="paramId" value="3"/>
      gfbrDL QOSFlowDescriptions,
      --<selector element="paramId" value="4"/>
      mfbrUL QOSFlowDescriptions,
      --<selector element="paramId" value="5"/>
      mfbrDL QOSFlowDescriptions,
      --<selector element="paramId" value="6"/>
      avgWindow QOSFlowDescriptions,
      --<selector element="paramId" value="7"/>
      resourceType AdditionalQoSInfo,
      --<selector element="paramId" value="8"/>
      priorityLevel AdditionalQoSInfo,
      --<selector element="paramId" value="9"/>
      packetDelayBudget AdditionalQoSInfo,
      --<selector element="paramId" value="10"/>
      packetErrRate AdditionalQoSInfo,
      --<selector element="paramId" value="11"/>
      maxDataBurstVol AdditionalQoSInfo,
      --<selector element="paramId" value="12"/>
      maxPktLossRateDL AdditionalQoSInfo,
      --<selector element="paramId" value="13"/>
      maxPktLossRateUL AdditionalQoSInfo,
      --<selector element="paramId" value="14"/>
      arp INTEGER (0..255),
      --<selector element="paramId" value="15"/>
      periodicity INTEGER (0..255)
   }
}

AdditionalQoSInfo ::= SEQUENCE {
   numParams INTEGER (0..255),
   --<inline/>
   --<length expr="@numParams"/>
   paramsList SEQUENCE OF AddlQoSParamsList
}

AddlQoSParamsList ::= SEQUENCE {
   paramId INTEGER (0..255),
   --<is3GLength/>
   length INTEGER (1..255),
   --<inline/>
   contents CHOICE {
      --<selector element="paramId" value="1"/>
      qosCharacteristics OCTET STRING (SIZE (10)),
      --<selector element="paramId" value="2"/>
      mfbrDL OCTET STRING (SIZE (3)),
      --<selector element="paramId" value="3"/>
      mfbrUL OCTET STRING (SIZE (3)),
      --<selector element="paramId" value="4"/>
      gfbrDL OCTET STRING (SIZE (3)),
      --<selector element="paramId" value="5"/>
      gfbrUL OCTET STRING (SIZE (3)),
      --<selector element="paramId" value="6"/>
      notificationCtrl NULL,
      --<selector element="paramId" value="7"/>
      maxPktLossRateDL OCTET STRING (SIZE (2)),
      --<selector element="paramId" value="8"/>
      maxPktLossRateUL OCTET STRING (SIZE (2))
   }
}

/* 9.11.4.37 - Non-3GPP delay budget - added in v18.4 */
Non3GDelayBudget ::= SEQUENCE {
   --<is3GLength bitFieldSize="16"/>
