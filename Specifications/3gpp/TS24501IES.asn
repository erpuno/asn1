UEIDTypes ::= ENUMERATED {
   upPrukId(1), cpPrukId(2), imei(3), imeisv(4), ueidTypeMAX(7)
}

RemoteUEProtocols ::= ENUMERATED {
   noIp(0), ipv4(1), ipv6(2), unstructured(4), ethernet(5), protMAX(7)
}

/* 9.11.4.30 - Requested MBS container - added in v17.4 */
RequestedMBSContainer ::= SEQUENCE {
   --<is3GLength bitFieldSize="16"/>
   length INTEGER (0..65535), -- Changed to 2 octets in v17.7
   --<is3GVarLenList lengthElem="length"/>
   --<storage>array</storage>
   sessionInfo SEQUENCE (SIZE (1..256)) OF MBSSessionInfo
}

MBSSessionInfo ::= SEQUENCE {
   spare INTEGER (0..15) DEFAULT 0,
   operation MBSOperations,
   type MBSSessionIdTypes,
   --<inline/>
   id CHOICE {
      --<selector element="type" value="1"/>
      tmgi TMGI,
      --<selector element="type" value="2"/>
      ipv4 SEQUENCE {
         srcAddrInfo OCTET STRING (SIZE (4)),
         destAddrInfo OCTET STRING (SIZE (4))
      },
      --<selector element="type" value="3"/>
      ipv6 SEQUENCE {
         srcAddrInfo OCTET STRING (SIZE (16)),
         destAddrInfo OCTET STRING (SIZE (16))
      }
   }
}

MBSOperations ::= ENUMERATED {
   join(1), leave(2), mbsopsMAX(3)
}

MBSSessionIdTypes ::= ENUMERATED {
   tmgi(1), ipv4(2), ipv6(3)
}

/* 9.11.4.31 - Received MBS container - added in v17.4 */
ReceivedMBSContainer ::= SEQUENCE {
   --<is3GLength bitFieldSize="16"/>
   length INTEGER (0..65535),
   --<is3GVarLenList lengthElem="length"/>
   receivedInfo SEQUENCE OF ReceivedMBSInfo
}

ReceivedMBSInfo ::= SEQUENCE {
   rejectionCause INTEGER (0..7),
   msai MSAITypes,
   md INTEGER (0..7),
   spare INTEGER (0..7) DEFAULT 0,
   ipat BOOLEAN,  -- Added in v17.9
   msci INTEGER (0..1),
   mti MTITypes,
   ipae INTEGER (0..1),
   tmgi TMGI,
   --<inline/>
   ipAddrInfo CHOICE {
      --<selector element="ipae" value="0"/>
      notIncluded NULL,
      --<selector element="ipae" value="1"/>
      included SEQUENCE {
         srcAddrInfo OCTET STRING (SIZE (4..16)),
         destAddrInfo OCTET STRING (SIZE (4..16))
      }
   },
   --<inline/>
   serviceArea CHOICE {
      --<selector element="msai" value="0"/>
      notIncluded NULL,
      --<selector element="msai" value="1"/>
      tai TrackingAreaIdentList,
      --<selector element="msai" value="2"/>
      --<length fixed-size="0"/>
      cgi SEQUENCE OF CGIList,
      --<selector element="msai" value="3"/>
      taiAndCgi SEQUENCE {
         tai TrackingAreaIdentList,
         --<length fixed-size="0"/>
         cgi SEQUENCE OF CGIList
      }
   },
   --<inline/>
   timer CHOICE {
      --<selector element="mti" value="0"/>
      notIncluded NULL,
      --<selector element="mti" value="1"/>
      startTime TimeZoneAndTime,
      --<selector element="mti" value="2"/>
      backoffTimer GPRSTimer
   },
   --<inline/>
   securityContainer CHOICE { -- Layout updated in v17.8
      --<selector element="msci" value="0"/>
      notIncluded NULL,
      --<selector element="msci" value="1"/>
      included SEQUENCE {
         numKeySets INTEGER (1..255),
         --<inline/>
         --<length expr="@.numKeySets"/>
         keySetList SEQUENCE (SIZE (1..255)) OF MBSSecurityKeySet
      }
   }
}

MSAITypes ::= ENUMERATED {
   notIncluded(0), inclTAI(1), inclCGI(2), inclTAICGI(3)
}

MTITypes ::= ENUMERATED {
   noTimers(0), startTime(1), backoffTimer(2), mtiMAX(3)
}

CGIList ::= SEQUENCE {
   nrCellId OCTET STRING (SIZE (5)),
   pair MCCMNCPair
}

MBSSecurityKeySet ::= SEQUENCE {
   spare INTEGER (0..127) DEFAULT 0,
   mtki INTEGER (0..1),
   keyDomainId MCCMNCPair,
   mskId OCTET STRING (SIZE (4)),
   msk OCTET STRING (SIZE (16)),
   --<inline/>
   mtkInfo CHOICE {
      --<selector element="mtki" value="0"/>
      notIncluded NULL,
      --<selector element="mtki" value="1"/>
      included SEQUENCE {
         mtkId OCTET STRING (SIZE (2)),
         mtk OCTET STRING (SIZE (16))
      }
   }
}

/* 9.11.4.32 - PDU session pair ID - added in v17.5 */
PDUSessionPairId ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255),
   id PDUPairIds
}

PDUPairIds ::= ENUMERATED {
   pairId0(0), pairId1(1), pairId2(2), pairId3(3), pairId4(4),
   pairId5(5), pairId6(6), ppiMAX(255)
}

/* 9.11.4.33 - RSN - added in v17.5 */
RSN ::= SEQUENCE {
   --<is3GLength/>
   length INTEGER (1..255),
   rsn INTEGER (0..255)
}

/* 9.11.4.34 - ECS address - added in v17.6 but not included in any messages as of v18.10 */
ECSAddress ::= SEQUENCE {
   --<is3GLength bitFieldSize="16"/>
   length INTEGER (0..65535),
   addressType ECSAddrTypes,
   spatialValidityType SpatialValidityTypes,
   --<inline/>
   address CHOICE {
      --<selector element="addressType" value="0"/>
      ipv4 OCTET STRING (SIZE (4)),
      --<selector element="addressType" value="1"/>
      ipv6 OCTET STRING (SIZE (16)),
      --<selector element="addressType" value="2"/>
      fqdn SEQUENCE {
         fqdnLen INTEGER (1..255),
         fqdnValue PrintableString
      },
      --<selector element="addressType" value="15"/>
      notIncluded NULL
   },
   --<noPresentBit/>
   contentsLen INTEGER (0..65535) OPTIONAL,
   --<noPresentBit/>
   --<inline/>
   contentsInfo CHOICE {
      --<selector element="spatialValidityType" value="0"/>
      none NULL,
      --<selector element="spatialValidityType" value="1"/>
      geoArea SEQUENCE {
         shapeType INTEGER (0..15),
         numPointsOrSpare INTEGER (0..15),
         description OCTET STRING (SIZE (6..90))
      },
      --<selector element="spatialValidityType" value="2"/>
      trackArea TrackingAreaIdent,
      --<selector element="spatialValidityType" value="3"/>
      country MCCMNCPair
   } OPTIONAL,
   --<noPresentBit/>
   spare INTEGER (0..63) OPTIONAL, -- Added in v18.7
   --<noPresentBit/>
   espili INTEGER (0..1) OPTIONAL, -- Added in v18.8
   --<noPresentBit/>
   eami INTEGER (0..1) OPTIONAL, -- Added in v18.7
   --<noPresentBit/>
   --<inline/>
   ecsAuthMethods CHOICE { -- Added in v18.7
      --<selector element="eami" value="0"/>
      absent NULL,
      --<selector element="eami" value="1"/>
      present SEQUENCE {
         spare INTEGER (0..31) DEFAULT 0,
         tlsgi BOOLEAN,
         tlsai BOOLEAN,
         tlscsci BOOLEAN
      }
   },
   --<noPresentBit/>
   --<inline/>
   ecsSupportedInfoList CHOICE { -- Added in v18.8
      --<selector element="espili" value="0"/>
      absent NULL,
      --<selector element="espili" value="1"/>
      present ECSSupportedInfoList
   }
}

ECSAddrTypes ::= ENUMERATED {
   eatIpv4(0), eatIpv6(1), eatFqdn(2), eatUnspecified(15)
}

SpatialValidityTypes ::= ENUMERATED {
   svtNone(0), svtGeographicalArea(1), svtTrackingArea(2), svtCountryWide(3),
   svtMAX(15)
}

ECSSupportedInfoList ::= SEQUENCE {
   --<is3GLength bitFieldSize="16"/>
   length INTEGER (0..65535),
   --<is3GVarLenList lengthElem="length"/>
   supportedInfo SEQUENCE OF SupportedPLMNInfo
}

SupportedPLMNInfo ::= SEQUENCE {
   --<is3GLength bitFieldSize="16"/>
   length INTEGER (0..65535),
   plmnId MCCMNCPair,
   --<is3GVarLenList lengthElem="length"/>
   ecspInfo SEQUENCE OF ECSPInfo
}

ECSPInfo ::= SEQUENCE {
   length INTEGER (0..65535),
   --<length fixed-size="0"/>
   ecspId IA5String
}

/* 9.11.4.36 - N3QAI - added in v18.3 */
N3QAI ::= SEQUENCE {
   --<is3GLength bitFieldSize="16"/>
   length INTEGER (0..65535),
