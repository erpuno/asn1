NRCellIdList ::= SEQUENCE {
   numIds INTEGER (1..255),
   --<inline/>
   --<length expr="@numIds"/>
   nrCellIds SEQUENCE OF NRCellId
}

NRCellId ::= SEQUENCE {
   plmnId MCCMNCPair,
   cellId BIT STRING (SIZE (36)),
   spare INTEGER (0..15) DEFAULT 0
}

RANNodeIdList ::= SEQUENCE {
   numIds INTEGER (1..255),
   --<inline/>
   --<length expr="@numIds"/>
   ranNodeIds SEQUENCE OF RANNodeId
