         spare INTEGER (0..15) DEFAULT 0,
         mode SSCMode
      },
      --<selector element="typeId" value="2"/>
      snssai SNSSAI,
      --<selector element="typeId" value="4"/>
      dnn DNNList,
      --<selector element="typeId" value="8"/>
      pduSession SEQUENCE {
         spare INTEGER (0..15) DEFAULT 0,
         session PDUSessionType
      },
      --<selector element="typeId" value="16"/>
      prefAccess SEQUENCE {
         spare INTEGER (0..15) DEFAULT 0,
         type AccessType
      },
      --<selector element="typeId" value="17"/>
      multiAccess NULL,
      --<selector element="typeId" value="32"/>
      nonSeamlessoffloadInd NULL,
      --<selector element="typeId" value="64"/>
      locCriteria LocationCriteria,
      --<selector element="typeId" value="128"/>
      timeWindow TimePeriod,
      --<selector element="typeId" value="129"/>
      proSeRelayOffloadInd NULL,
      --<selector element="typeId" value="130"/>
      pduSessionPairId PDUPairIds,
      --<selector element="typeId" value="131"/>
      rsn INTEGER (0..255),
      --<selector element="typeId" value="132"/>
      proSeMultiPathPref NULL
