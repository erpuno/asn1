}

RouteSelectionDescriptorList ::= SEQUENCE {
   --<is3GLength bitFieldSize="16"/>
   length INTEGER (0..65535),
   --<is3GVarLenList lengthElem="length"/>
   descriptors SEQUENCE OF RouteSelectionDescriptor
}

RouteSelectionDescriptor ::= SEQUENCE {
   --<is3GLength bitFieldSize="16"/>
   length INTEGER (0..65535),
   precedence INTEGER (0..255),
   --<is3GVarLenList lengthElem="length"/>
   contents SEQUENCE OF RouteSelectionDescriptorContents
}

RouteSelectionDescriptorContents ::= SEQUENCE {
   --<is3GLength bitFieldSize="16"/>
   length INTEGER (0..65535),
   --<is3GVarLenList lengthElem="length"/>
   components SEQUENCE OF RouteSelectionDescriptorComponent
