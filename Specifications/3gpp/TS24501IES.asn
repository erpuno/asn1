      --<selector element="type" value="2"/>
      nrCellIdList NRCellIdList,
      --<selector element="type" value="3"/>
      ranNodeIdList RANNodeIdList,
      --<selector element="type" value="4"/>
      taiList TrackingAreaIdentList
   }
}

EUTRACellIdList ::= SEQUENCE {
   numIds INTEGER (0..255),
   --<inline/>
   --<length expr="@numIds"/>
   eutraCellIds SEQUENCE OF EUTRACellId
}

EUTRACellId ::= SEQUENCE {
   plmnId MCCMNCPair,
   cellId BIT STRING (SIZE (28)),
   spare INTEGER (0..15) DEFAULT 0
