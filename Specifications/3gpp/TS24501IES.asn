RTPPayloadInfoList ::= SEQUENCE {
   --<is3GLength bitFieldSize="16"/>
   length INTEGER (0..65535),
   --<is3GVarLenList lengthElem="length"/>
   info SEQUENCE OF RTPPayloadInfo
}

RTPPayloadInfo ::= SEQUENCE {
   format INTEGER (0..255),
   numTypes INTEGER (0..255),
   --<inline/>
   --<length expr="@numTypes"/>
   types SEQUENCE OF OCTET STRING
}

