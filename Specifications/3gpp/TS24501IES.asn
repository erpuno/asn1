   rtpHdrInfo CHOICE {
      --<selector element="heipi" value="0"/>
      absent NULL,
      --<selector element="heipi" value="1"/>
      hdrInfo SEQUENCE {
         type INTEGER (0..255),
         id INTEGER (0..255)
