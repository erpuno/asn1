   qfiInfo CHOICE {
      --<selector element="qfipi" value="0"/>
      absent NULL,
      --<selector element="qfipi" value="1"/>
      present SEQUENCE {
         numQFIs INTEGER (1..255),
         --<length expr="@.numQFIs"/>
         qfiList SEQUENCE OF OCTET STRING
      }
