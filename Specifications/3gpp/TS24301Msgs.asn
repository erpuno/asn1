   obj-ActvDedEPSBearerCtxtAcc | obj-ActvDedEPSBearerCtxtRej |
   obj-ActvDedEPSBearerCtxtReq | obj-ActvDfltEPSBearerCtxtAcc |
   obj-ActvDfltEPSBearerCtxtRej | obj-ActvDfltEPSBearerCtxtReq |
   obj-BearerResAllocRej | obj-BearerResAllocReq | obj-BearerResModRej |
   obj-BearerResModReq | obj-DeactEPSBearerCtxtAcc |
   obj-DeactEPSBearerCtxtReq | obj-ESMInfoReq | obj-ESMInfoResp |
   obj-ESMStatus | obj-ModEPSBearerCtxtAcc | obj-ModEPSBearerCtxtRej |
   obj-ModEPSBearerCtxtReq | obj-PDNConnectRej | obj-PDNConnectReq |
   obj-PDNDisconnRej | obj-PDNDisconnReq | obj-ESMDataTransport |
   obj-RemoteUEReport
}

PDU-SM ::= SEQUENCE {
   l3HdrOpts  L3HdrOptions, -- L3 header, octet 1, bits 5 thru 8
   --<cDecSrcFname>PDU_protoDiscr.c</cDecSrcFname>
   protoDiscr NAS-PROTOCOL-CLASS.&protoDiscr ({TS24301-IE-SM-OBJECTSET}), 
   --<inline/>
   pti CHOICE {
      --<selector element="protoDiscr" value="2"/>
      value INTEGER (0..255), 
      noValue NULL
   }, 
   msgType NAS-PROTOCOL-CLASS.&msgType ({TS24301-IE-SM-OBJECTSET}), 
   data    NAS-PROTOCOL-CLASS.&Value 
              ({TS24301-IE-SM-OBJECTSET}{@protoDiscr,@msgType})
}


/* 9.9.3.15 - ESM message container

   This is defined here rather than the IES ASN file because of the PDU-SM
   type above.  If this were in the IES file, it caused a circular reference
   issue, which changed some generated types' structs to pointers to those
   structs.  This created a backward compatability issue.  To get around that,
   it is defined here...
*/
ESMMessageContainer ::= SEQUENCE {
   --<is3GLength bitFieldSize="16"/>
   length INTEGER (0..65535),
   pduSm PDU-SM
}


